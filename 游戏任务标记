#include<iostream>
using namespace std;
int main(){
    int a,b,i,e=-1;
    unsigned int s[1024];
    while(cin>>a>>b){
        

    for(i=0;i<1024;i++){
        s[i]=0;
    }
    if(a>0&&a<1025&&b>0&&b<1025){
        s[a-1]=1;
        cout<<s[b-1]<<endl;
    }
    else cout<<e<<endl;   
    }
    return 0;
}
