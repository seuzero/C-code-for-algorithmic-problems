#include<iostream>
using namespace std;
int main(){
    int n,a=-90,b=90;
    while(cin>>n){
        while(b-a>=5){
            if(n-a>=b-n||(b+a)/2==n){
                a=(b+a)/2;
                cout<<'1';
            }
             
        else {
            b=(b+a)/2;
            cout<<'0';
        }
        }
 
    }
    return 0;
}
